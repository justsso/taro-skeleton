{
	"name": "taro-skeleton",
	"version": "2.0.3",
	"description": "基于Taro的一个骨架屏组件，简单易用",
	"main:h5": "dist/h5/index.js",
	"browser": "dist/index.umd.js",
	"module": "dist/index.esm.js",
	"main": "dist/index.js",
	"source": "src/components/Skeleton/index.tsx",
	"keywords": [
		"taro",
		"taro-skeleton",
		"skeleton"
	],
	"types": "./types/index.d.ts",
	"templateInfo": {
		"name": "default",
		"typescript": true,
		"css": "sass"
	},
	"files": [
		"dist",
		"types",
		"src/**/interface.ts",
		"src/**/*.d.ts"
	],
	"pre-commit": {
		"run": "build:rollup"
	},
	"scripts": {
		"build:weapp": "taro build --type weapp",
		"build:swan": "taro build --type swan",
		"build:alipay": "taro build --type alipay",
		"build:tt": "taro build --type tt",
		"build:h5": "taro build --type h5",
		"build:rn": "taro build --type rn",
		"build:qq": "taro build --type qq",
		"build:quickapp": "taro build --type quickapp",
		"build:ui": "TARO_BUILD_TYPE=ui taro build --ui",
		"build:rollup": "rollup --config ./config/rollup.config.js",
		"dev:weapp": "npm run build:weapp -- --watch",
		"dev:swan": "npm run build:swan -- --watch",
		"dev:alipay": "npm run build:alipay -- --watch",
		"dev:tt": "npm run build:tt -- --watch",
		"dev:h5": "npm run build:h5 -- --watch",
		"dev:rn": "npm run build:rn -- --watch",
		"dev:qq": "npm run build:qq -- --watch",
		"dev:quickapp": "npm run build:quickapp -- --watch",
		"update": "taro update project 2.0.0",
		"commit": "git-cz",
		"release": "release-it --no-git.requireUpstream",
		"changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
	},
	"author": "lentoo",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git@github.com:lentoo/taro-skeleton.git"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	},
	"dependencies": {
		"@tarojs/components": "3.0.11",
		"@tarojs/router": "3.0.11",
		"@tarojs/taro": "3.0.11",
		"@tarojs/taro-alipay": "2.1.0",
		"@tarojs/taro-h5": "3.0.11",
		"@tarojs/taro-qq": "2.1.0",
		"@tarojs/taro-quickapp": "2.1.0",
		"@tarojs/taro-swan": "2.1.0",
		"@tarojs/taro-tt": "2.1.0",
		"@tarojs/taro-weapp": "2.1.0",
		"classnames": "^2.2.6",
		"nerv-devtools": "^1.5.7",
		"nervjs": "^1.5.7",
		"react": "^16.13.1",
		"react-dom": "^16.13.1"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^14.0.0",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^8.4.0",
		"@tarojs/mini-runner": "3.0.11",
		"@tarojs/plugin-babel": "2.1.0",
		"@tarojs/plugin-csso": "2.1.0",
		"@tarojs/plugin-sass": "2.1.0",
		"@tarojs/plugin-uglifyjs": "2.1.0",
		"@tarojs/webpack-runner": "3.0.11",
		"@types/classnames": "^2.2.9",
		"@types/react": "^16.4.6",
		"@types/webpack-env": "^1.13.6",
		"@typescript-eslint/parser": "^1.6.0",
		"babel-eslint": "^8.2.3",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-decorators-legacy": "^1.3.4",
		"babel-plugin-transform-jsx-stylesheet": "^0.6.5",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"conventional-changelog-cli": "^2.0.23",
		"cz-conventional-changelog": "^3.0.2",
		"eslint": "^5.16.0",
		"eslint-config-taro": "3.0.11",
		"eslint-plugin-import": "^2.12.0",
		"eslint-plugin-react": "^7.8.2",
		"eslint-plugin-react-hooks": "^2.0.1",
		"eslint-plugin-taro": "3.0.11",
		"git-cz": "^3.2.1",
		"pre-commit": "^1.2.2",
		"release-it": "^12.3.5",
		"rollup": "^2.23.1",
		"rollup-plugin-copy": "^3.3.0",
		"rollup-plugin-scss": "^2.5.0",
		"rollup-plugin-typescript2": "^0.27.2",
		"rollup-plugin-visualizer": "^4.1.0",
		"stylelint": "9.3.0",
		"stylelint-config-taro-rn": "2.1.0",
		"stylelint-taro-rn": "2.1.0",
		"typescript": "^3.9.7"
	}
}
